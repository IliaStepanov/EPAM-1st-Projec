CREATE TABLE USERS(id LONG AUTO_INCREMENT NOT NULL, email VARCHAR(255), password VARCHAR(255), isAdmin BOOL,firstName VARCHAR(255), lastName VARCHAR(255), documentInfo VARCHAR(255), birthday DATE, PRIMARY KEY (id));
